    {"date": "2026-09-13", "ref": "43:8:31-32", "note": "Bli i mitt ord"},
    {"date": "2026-09-14", "ref": "58:12:14", "note": "Streb etter fred og helliggjørelse"},
    {"date": "2026-09-15", "ref": "19:55:23", "note": "Kast din byrde på Herren"},
    {"date": "2026-09-16", "ref": "40:5:6", "note": "Hungrer etter rettferdighet"},
    {"date": "2026-09-17", "ref": "45:13:11-12", "note": "Dagen er nær"},
    {"date": "2026-09-18", "ref": "19:4:9", "note": "Trygg i hvile"},
