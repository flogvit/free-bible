    {"date": "2026-09-25", "ref": "43:13:14-15", "note": "Tjener for hverandre"},
    {"date": "2026-09-26", "ref": "58:4:12", "note": "Guds ord er levende"},
    {"date": "2026-09-27", "ref": "19:122:1", "note": "Glad over å gå til Herrens hus"},
    {"date": "2026-09-28", "ref": "40:18:3", "note": "Bli som barn"},
    {"date": "2026-09-29", "ref": "45:8:6", "note": "Åndens sinnelag er liv og fred"},
    {"date": "2026-09-30", "ref": "19:45:2", "note": "Skjønnere enn alle mennesker"},
