    {"date": "2026-08-10", "ref": "19:105:1-4", "note": "Pris Herren"},
    {"date": "2026-08-11", "ref": "40:8:26-27", "note": "Hvorfor er dere redde?"},
    {"date": "2026-08-12", "ref": "58:13:5-6", "note": "Aldri forlate deg"},
    {"date": "2026-08-13", "ref": "19:71:5-6", "note": "Du er mitt håp"},
    {"date": "2026-08-14", "ref": "43:10:14-15", "note": "Jeg kjenner mine"},
    {"date": "2026-08-15", "ref": "45:13:8", "note": "Elsk hverandre"},
    {"date": "2026-08-16", "ref": "19:48:2", "note": "Stor er Herren"},
    {"date": "2026-08-17", "ref": "40:25:34-36", "note": "Hjelpe de minste"},
    {"date": "2026-08-18", "ref": "49:3:16-17", "note": "Kristus bo i hjertene"},
    {"date": "2026-08-19", "ref": "19:50:15", "note": "Rop til meg i nøden"},
    {"date": "2026-08-20", "ref": "43:14:12", "note": "Større gjerninger"},
    {"date": "2026-08-21", "ref": "58:10:35-36", "note": "Kast ikke bort frimodigheten"},
    {"date": "2026-08-22", "ref": "19:52:10", "note": "Trygg som oliventre"},
    {"date": "2026-08-23", "ref": "40:6:24", "note": "Kan ikke tjene to herrer"},
    {"date": "2026-08-24", "ref": "45:12:17-18", "note": "Hold fred med alle"},
    {"date": "2026-08-25", "ref": "19:119:9-11", "note": "Hvordan holde veien ren"},
    {"date": "2026-08-26", "ref": "43:9:25", "note": "En ting vet jeg"},
    {"date": "2026-08-27", "ref": "49:6:18", "note": "Be alltid i Ånden"},
    {"date": "2026-08-28", "ref": "19:69:31", "note": "Lovsang gleder Gud"},
    {"date": "2026-08-29", "ref": "40:16:18-19", "note": "På denne klippe"},
    {"date": "2026-08-30", "ref": "45:8:31-32", "note": "Er Gud for oss"},
    {"date": "2026-08-31", "ref": "19:28:7", "note": "Herren er min styrke"},
    {"date": "2026-09-01", "ref": "43:1:12", "note": "Rett til å bli Guds barn"},
    {"date": "2026-09-02", "ref": "58:11:13-16", "note": "Troens vitner"},
    {"date": "2026-09-03", "ref": "19:126:1-3", "note": "Store ting har Herren gjort"},
    {"date": "2026-09-04", "ref": "40:24:35", "note": "Mine ord skal ikke forgå"},
    {"date": "2026-09-05", "ref": "45:1:20", "note": "Guds usynlige vesen"},
    {"date": "2026-09-06", "ref": "19:117:1-2", "note": "Alle folk skal prise"},
    {"date": "2026-09-07", "ref": "43:16:22", "note": "Glede som ingen tar fra dere"},
    {"date": "2026-09-08", "ref": "49:2:10", "note": "Skapt til gode gjerninger"},
    {"date": "2026-09-09", "ref": "19:61:3-4", "note": "Før meg til klippen"},
    {"date": "2026-09-10", "ref": "40:6:6", "note": "Be i lønnkammeret"},
    {"date": "2026-09-11", "ref": "45:12:21", "note": "Overvin det onde med det gode"},
